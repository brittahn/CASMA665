   ],
   "source": [
    "import numpy as  np\n",
    "import matplotlib.pyplot as plt  # plotting\n",
    "\n",
    "x = np.linspace(0,10,11)   \n",
    "print( \"x = \" + str(x) )\n",
    "\n",
    "#   A vector that starts at 0, ends at 10, and has 11 entries (takes steps of size 1 from 0 to 10)\n",
    "\n",
    "y = np.sin(x)\n",
    "print( \"y = \" + str(y) )\n",
    "print(\"x, y sine\")\n",
    "plt.figure()\n",
    "plt.plot(x,y)\n",
    "plt.show()\n",
    "\n",
    "# make a smoother curve by adding more x data points\n",
    "x_smooth = np.linspace(0, 10, 100)\n",
    "y_smooth = np.sin(x_smooth)   # x and y must have the same number of points\n",
    "print(\"\\n\\n x, y smoother sine\")\n",
    "plt.figure()\n",
    "plt.plot(x_smooth,y_smooth)\n",
    "plt.show()"
   ]
  },
